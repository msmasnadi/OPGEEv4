            <Process class="PostStorageCompressor"/>
        </Aggregator>

		<Aggregator name="LiquifiedNaturalGas">
            <Process class="NGL"/>
            <Process class="LNGLiquefaction"/>
            <Process class="LNGRegasification"/>
            <Process class="LNGTransport"/>
