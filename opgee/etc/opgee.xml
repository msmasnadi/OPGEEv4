			</ProcessGroup>

			<ProcessGroup name="Sour Gas Reinjection">
				<ProcessRef name="GasGathering"/>
				<ProcessRef name="GasDehydration"/>
				<ProcessRef name="SourGasCompressor"/>
				<ProcessRef name="SourGasInjection"/>
